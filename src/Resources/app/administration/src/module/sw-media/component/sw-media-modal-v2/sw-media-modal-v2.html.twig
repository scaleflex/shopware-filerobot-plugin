{% block sw_media_modal_v2_tab_items %}
    {% parent %}
    <sw-tabs-item
            :name="tabFilerobotDAM"
            :active-tab="active"
    >
        Filerobot DAM
    </sw-tabs-item>
{% endblock %}

{% block sw_media_modal_v2_tab_content_library %}
    {% parent %}
    <template v-if="active === tabFilerobotDAM">
        <sw-filerobot-library
                ref="filerobotLibrary"
                @media-selection-change="selection = $event"
        />
    </template>
{% endblock %}
